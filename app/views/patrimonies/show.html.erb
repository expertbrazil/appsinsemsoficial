<div class="page-header">
  <%= link_to patrimonies_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_patrimony_path(@patrimony), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show patrimony</h1>
</div>

<dl class="dl-horizontal">
  <dt>Department:</dt>
  <dd><%= @patrimony.department %></dd>

  <dt>Name:</dt>
  <dd><%= @patrimony.name %></dd>

  <dt>Description:</dt>
  <dd><%= @patrimony.description %></dd>

  <dt>Quantity:</dt>
  <dd><%= @patrimony.quantity %></dd>

  <dt>Date of acquisition:</dt>
  <dd><%= @patrimony.date_of_acquisition %></dd>

  <dt>Amount:</dt>
  <dd><%= @patrimony.amount %></dd>

  <dt>Patrimony number:</dt>
  <dd><%= @patrimony.patrimony_number %></dd>

</dl>
