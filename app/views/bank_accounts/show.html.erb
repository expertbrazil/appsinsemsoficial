<div class="page-header">
  <%= link_to bank_accounts_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_bank_account_path(@bank_account), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show bank_account</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @bank_account.name %></dd>

  <dt>Agency:</dt>
  <dd><%= @bank_account.agency %></dd>

  <dt>Account number:</dt>
  <dd><%= @bank_account.account_number %></dd>

  <dt>Cedente:</dt>
  <dd><%= @bank_account.cedente %></dd>

  <dt>Cedente doc:</dt>
  <dd><%= @bank_account.cedente_doc %></dd>

  <dt>Cedente address:</dt>
  <dd><%= @bank_account.cedente_address %></dd>

  <dt>Variacao:</dt>
  <dd><%= @bank_account.variacao %></dd>

  <dt>Aceite:</dt>
  <dd><%= @bank_account.aceite %></dd>

  <dt>Carteira:</dt>
  <dd><%= @bank_account.carteira %></dd>

  <dt>Convenio:</dt>
  <dd><%= @bank_account.convenio %></dd>

  <dt>Instrucao juros:</dt>
  <dd><%= @bank_account.instrucao_juros %></dd>

  <dt>Default bank:</dt>
  <dd><%= @bank_account.default_bank %></dd>

  <dt>Layout:</dt>
  <dd><%= @bank_account.layout %></dd>

</dl>
