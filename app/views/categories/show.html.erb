<div class="page-header">
  <%= link_to categories_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_category_path(@category), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show category</h1>
</div>

<dl class="dl-horizontal">
  <dt>Subcategory:</dt>
  <dd><%= @category.subcategory %></dd>

  <dt>Type:</dt>
  <dd><%= @category.type_category %></dd>

  <dt>Name:</dt>
  <dd><%= @category.name %></dd>

  <dt>Status:</dt>
  <dd><%= @category.status %></dd>

</dl>
