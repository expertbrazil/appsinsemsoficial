<div class="page-header">
  <%= link_to suppliers_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_supplier_path(@supplier), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show supplier</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @supplier.name %></dd>

  <dt>Phone:</dt>
  <dd><%= @supplier.phone %></dd>

  <dt>Email:</dt>
  <dd><%= @supplier.email %></dd>

</dl>
