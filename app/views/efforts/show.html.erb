<div class="page-header">
  <%= link_to efforts_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_effort_path(@effort), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show effort</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @effort.name %></dd>

  <dt>To describe:</dt>
  <dd><%= @effort.to_describe %></dd>

</dl>
