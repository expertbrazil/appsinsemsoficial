}
#footer {
    width: 100%;
    margin: auto;
    height: 22px;
    clear: both;
    /*border-top: 20px solid #fff;*/
    position: relative;
    z-index: 2;
}
#content {
    vertical-align: middle;
    display: table-cell;
    padding: 128px 0 0;
}
#content .inner {
    /*background: blue;*/
    padding: 0 10px;
    /*width: 300px;*/
    /*height: 200px;*/
    margin: 0 auto;
}

@page {
  size: A4;
  margin: 0;
}
@media print{    
  html, body {
      width: 210mm;
      height: 297mm;
  }
    .no-print, .no-print *
    {
        display: none !important;
    }
}

