<div class="card">
	<div class="card-header ch-alt m-b-20">  
		<%= link_to professionals_path, "data-toggle": "tooltip", "data-placement":"bottom", "title": "Voltar", class: 'btn bgm-deeporange btn-float waves-effect' do %>
		<i class="zmdi zmdi-arrow-left"></i>
		<% end  if false%>

		<%= link_to @professional, class: 'btn btn-primary waves-effect' do %>
