<div class="page-header">
  <%= link_to professionals_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_professional_path(@professional), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show professional</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @professional.name %></dd>

  <dt>Cpf:</dt>
  <dd><%= @professional.cpf %></dd>

  <dt>Rg:</dt>
  <dd><%= @professional.rg %></dd>

  <dt>Advice:</dt>
  <dd><%= @professional.advice %></dd>

  <dt>Type function:</dt>
  <dd><%= @professional.type_function %></dd>

</dl>
