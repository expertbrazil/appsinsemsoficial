<div class="page-header">
  <%= link_to companies_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_company_path(@company), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show company</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @company.name %></dd>

  <dt>Email:</dt>
  <dd><%= @company.email %></dd>

  <dt>Address:</dt>
  <dd><%= @company.address %></dd>

  <dt>Number:</dt>
  <dd><%= @company.number %></dd>

  <dt>Complement:</dt>
  <dd><%= @company.complement %></dd>

  <dt>Zipcode:</dt>
  <dd><%= @company.zipcode %></dd>

  <dt>Burgh:</dt>
  <dd><%= @company.burgh %></dd>

  <dt>City:</dt>
  <dd><%= @company.city %></dd>

  <dt>State:</dt>
  <dd><%= @company.state %></dd>

  <dt>Phone:</dt>
  <dd><%= @company.phone %></dd>

  <dt>Benefit:</dt>
  <dd><%= @company.benefit %></dd>

</dl>
