<div class="card">
	<div class="card-header ch-alt m-b-20">  
		<%= link_to monthly_payments_path, "data-toggle": "tooltip", "data-placement":"bottom", "title": "Voltar", class: 'btn bgm-deeporange btn-float waves-effect' do %>
		<i class="zmdi zmdi-arrow-left"></i>
		<% end  if false%>

		<%= link_to @monthly_payment, class: 'btn btn-primary waves-effect' do %>
		    <span class="glyphicon glyphicon-info-sign"></span>
		    Show
		<% end if false%>

		<h2><i class="fa fa-calendar"></i> <%= "Editando mensalidade" %></h2>
	</div>

	<%= render 'form' %>
</div>
