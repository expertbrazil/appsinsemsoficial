<div class="page-header">
  <%= link_to payment_forms_path, class: 'btn btn-default waves-effect' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_payment_form_path(@payment_form), class: 'btn btn-primary waves-effect' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show payment_form</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @payment_form.name %></dd>

  <dt>Status:</dt>
  <dd><%= @payment_form.status %></dd>

</dl>
